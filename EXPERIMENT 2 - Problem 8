#include <conio.h>
#include <iostream>
using namespace std;
int main()
{
    int Numbers;
    cout<<"Enter a Number: ";
    cin>>Numbers;
    cout<<"Multiplication Table of: ";
    cin>>Numbers;
    
    for (int i=1; i<=10; i++)
    {
        cout<<i<<"*"<<Numbers<<"="<<i*Numbers<<endl;
        
    }
_getch();
return 0;
}
